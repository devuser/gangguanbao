{
  "name": "gangguanbao-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--no-deprecation' vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,vue,js,css,json}\" \"index.html\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,js,css,json}\" \"index.html\""
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "axios": "^1.6.0",
    "echarts": "^5.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0",
    "prettier": "^3.2.0"
  }
}
